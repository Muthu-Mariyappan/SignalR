{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "DefaultPolicyName": "OnlyFromLocalHost",
    "OnlyFromLocalHost": {
      "AllowedOrigins": "http://localhost:4200",
      "AllowedHeaders": "*",
      "AllowedMethods": "GET, POST, DELETE, PUT, HEAD"
    }
  }
}